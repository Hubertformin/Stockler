{
  "name": "stockler",
  "version": "1.0.1",
  "description": "Stock Management",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "package-win": "electron-packager . Stockler --overwrite --asar=true --platform=win32 --arch=ia32 --icon=logo.ico --prune=true --out=release-builds --version-string.CompanyName=\"Silverslopescm \" --version-string.FileDescription=\"Stockler\" --version-string.ProductName=\"Stockler\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hubertformin/Stockler.git"
  },
  "author": "Hubert Formin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hubertformin/Stockler/issues"
  },
  "build": {
    "appId": "your.id",
    "productName": "Stockler",
    "copyright": "Copyright Â© 2018 Silverslopes",
    "win": {
      "icon": "logo.ico"
    },
    "fileAssociations": {
      "ext": [
        "stk"
      ],
      "name": "STK",
      "description": "Stockler Logs Extension",
      "icon": "stk-icon.ico"
    }
  },
  "homepage": "https://github.com/Hubertformin/Stockler#readme",
  "devDependencies": {
    "electron": "^3.0.6",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "cryptr": "^4.0.0",
    "dexie": "^2.0.4",
    "electron-regedit": "^1.0.6",
    "electron-settings": "^3.2.0",
    "file-saverjs": "^1.3.6",
    "ftp": "^0.3.10",
    "html-pdf": "^2.2.0",
    "jquery": "^3.3.1",
    "tableexport": "^5.0.3",
    "zlib": "^1.0.5"
  }
